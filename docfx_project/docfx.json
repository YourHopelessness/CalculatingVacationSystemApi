{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [
            "CalculationVacationSystem.BL/**.csproj",
	    "CalculationVacationSystem.DAL/**.csproj",
            "CalculationVacationSystem.WebApi/**.csproj"
          ],
	   "exclude": [
            	"**/bin/**",
           	 "**/obj/**",
            	"_site/**"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
         "files": [
	  "images/**",
          "images/favicon.ico"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "./"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}